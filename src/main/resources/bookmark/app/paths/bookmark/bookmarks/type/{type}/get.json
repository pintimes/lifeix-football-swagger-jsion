{
    "summary": "获取用户的是否收藏过某targets",
    "tags": 
    [
        "bookmarks"
    ],

    "consumes": 
    [
        "application/json; charset=UTF-8"
    ],

    "produces": 
    [
        "application/json; charset=UTF-8"
    ],

    "description": "通过传入类型以及ids获得当前用户是否收藏过这些Targets，一般用于需要获得某实体列表的收藏情况",
    "parameters": 
    [
        {
            "$ref": "#/parameters/apiKeyYHeader"
        },

        {
            "name": "type",
            "in": "path",
            "type": "string",
            "required": true,
            "description": "参见Bookmark"
        },

        {
            "name": "ids",
            "in": "query",
            "type": "string",
            "required": true,
             "description": "收藏的对象Id，比如postId，videoId，……Id"
        }
    ],

    "responses": 
    {
        "200": 
        {
            "description": "Bookmarks对象",
            "schema": 
            {
                "type": "array",
                "items": 
                {
                   "$ref": "#/definitions/Bookmark"
                }
            }
        },
        
        "401": 
        {
            "description": "未授权"
        }
    },

    "operationId": "getTargetBookmarks"
}