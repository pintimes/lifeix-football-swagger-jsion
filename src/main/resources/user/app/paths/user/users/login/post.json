{
    "summary": "普通用户登录",
    "tags": 
    [
        "登录"
    ],

    "description": "普通用户登录",
    
    "produces": 
    [
        "application/json"
    ],

    "consumes": 
    [
        "application/x-www-form-urlencoded"
    ],

    "parameters": 
    [
    	{
            "$ref": "#/parameters/apiKeyHeader"
        },
        {
            "name": "source",
            "type": "string",
            "required": false,
            "description": "来源，competition/c-f，内部系统调用时才需要该参数",
            "in": "formData"
        },
        {
            "name": "device",
            "type": "integer",
            "format": "int32",
            "required": true,
            "description": "设备类型，1：web，2：ios，3：android",
            "in": "formData"
        },
        {
            "name": "deviceId",
            "type": "string",
            "required": false,
            "description": "设备号，web不需要传该参数，其他平台需要传该参数",
            "in": "formData"
        },
        {
            "name": "deviceToken",
            "type": "string",
            "required": false,
            "description": "",
            "in": "formData"
        },
        {
            "name": "account",
            "type": "string",
            "required": true,
            "description": "账号（用户名/手机号/邮箱）",
            "in": "formData"
        },
        {
            "name": "password",
            "type": "string",
            "required": true,
            "description": "密码（明文）",
            "in": "formData"
        }

    ],

    "responses": 
    {
        "200": 
        {
            "schema": 
            {
                "$ref": "#/definitions/User"
            },

            "description": "User对象"
        }
    },

    "operationId": "loginUser"
}