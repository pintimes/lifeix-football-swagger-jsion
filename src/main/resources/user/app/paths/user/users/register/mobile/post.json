{
    "summary": "用户手机注册",
    "tags": 
    [
        "手机注册"
    ],

    "description": "用户手机注册",
    
    "produces": 
    [
        "application/json"
    ],

    "consumes": 
    [
        "application/x-www-form-urlencoded"
    ],

    "parameters": 
    [
    	{
            "$ref": "#/parameters/apiKeyHeader"
        },
        {
            "name": "device",
            "type": "integer",
            "format": "int32",
            "required": true,
            "description": "设备类型，1：web，2：ios，3：android",
            "in": "formData"
        },
        {
            "name": "deviceId",
            "type": "string",
            "required": false,
            "description": "设备号，web不需要传该参数，其他平台需要传该参数",
            "in": "formData"
        },
        {
            "name": "deviceToken",
            "type": "string",
            "required": false,
            "description": "",
            "in": "formData"
        },
        {
            "name": "mobile",
            "type": "string",
            "required": true,
            "description": "手机号",
            "in": "formData"
        },
        {
            "name": "password",
            "type": "string",
            "required": true,
            "description": "密码（明文），至少为8位",
            "in": "formData"
        },
        {
            "name": "smsCode",
            "type": "string",
            "required": true,
            "description": "短信验证码",
            "in": "formData"
        },
        {
            "name": "source",
            "type": "string",
            "required": true,
            "description": "注册来源（c-f/competition）",
            "in": "formData"
        }

    ],

    "responses": 
    {
        "200": 
        {
            "schema": 
            {
                "$ref": "#/definitions/User"
            },

            "description": "User对象"
        },
        "409": 
        {
            "schema": 
            {
                "$ref": "#/definitions/User"
            },

            "description": "手机号已注册"
        }
    },

    "operationId": "mobileRegister"
}