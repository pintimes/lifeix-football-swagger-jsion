{
    "summary": "分页获取某个分类下的单篇列表",
    "tags": 
    [
    	"getElearningPageList"
    ],

    "description": "分页获取某个分类下的单篇列表",
    
    "produces": 
    [
        "application/json"
    ],
    
    "parameters": 
    [
    	{
            "$ref": "#/parameters/categoryPath"
        },
        {
            "description": "分类下存在多个单篇，分页加载更多时，把上一次请求的单篇列表中最后一个page的pageIndex作为起始索引，不传默认返回第一页数据",
            "name": "loadMorePageIndex",
            "type": "integer",
            "required": false,
            "in": "query"
        },
        {
            "description": "每页显示单篇的数量，默认值20",
            "name": "pageLimit",
            "type": "integer",
            "required": false,
            "in": "query"
        },
		{
            "description": "每个单篇显示的视频数量，默认值20",
            "name": "videoLimit",
            "type": "integer",
            "required": false,
            "in": "query"
        }
    ],

    "responses": 
    {
        "200": 
        {
			"schema": 
            {
                "type": "array",
	            "items": 
	            {
	                "$ref": "#/definitions/ElearningPage"
	            },
            },
            "description": "单篇列表"
        }
    },

    "operationId": "getElearningPageList"
}