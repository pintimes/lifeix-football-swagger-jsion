{
    "summary": "获取喜欢列表",
    "tags": 
    [
        "likes"
    ],

    "description": "将type=${type}的所有目标的喜欢情况返回，同时加入当前喜欢与否情况",
    "produces": 
    [
        "application/json"
    ],

    "consumes": 
    [
        "application/json"
    ],

    "parameters": 
    [
        {
            "$ref": "#/parameters/apiKeyYHeader"
        },

        {
            "name": "type",
            "in": "query",
            "type": "string",
            "required": true,
            "description": "target的类型，如果是国家队成员则写nationalteam，如果是文章则填入post"
        }
    ],

    "responses": 
    {
        "200": 
        {
            "schema": 
            {
                "type": "array",
                "items": 
                {
                    "$ref": "#/definitions/Like"
                }
            },

            "description": "Like列表响应对象"
        }
    },

    "operationId": "getLikes"
}