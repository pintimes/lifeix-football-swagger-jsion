            "type": "string",
            "format":"int32"
        },
        "uploadProgress": {
        	"description": "文件上传进度，0~1之间的整数",
            "type": "string",
            "format": "int32"
        },
        "remarks": {
        	"description": "备注",
            "type": "string"
        },
        "authorId": {
        	"description": "作者id",
            "type": "string"
        },
        "constringent": {
        	"description": "是否执行过压缩操作",
            "type": "string",
            "format": "boolean"
        },
        "watermarked": {
        	"description": "是否执行过打水印操作",
            "type": "string",
            "format": "boolean"
