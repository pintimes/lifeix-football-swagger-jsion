{
    "summary": "删除置顶资讯",
    "tags": 
    [
        "WemediaTop"
    ],

    "description": "根据类目id和置顶资讯id删除某类目下的某条置顶资讯",
    
    "consumes": 
    [
        "application/x-www-form-urlencoded"
    ],
    
    "produces": 
    [
        "text/plain; charset=UTF-8"
    ],

    "parameters": 
    [
        {
            "$ref": "#/parameters/apiKeyYHeader"
        },

        {
            "description": "类目Id",
            "name": "categoryId",
            "type": "string",
            "required": true,
            "in": "formData"
        },

        {
            "description": "置顶资讯对应的资讯id列表",
            "name": "postIds",
            "type": "array",
            "items": 
            {
                "type": "string"
            },
            "required": true,
            "in": "formData"
        }
    ],

    "responses": 
    {
        "200": 
        {
            "description": "删除成功"
        },
        
        "401": 
        {
            "description": "没有权限"
        }
    },

    "operationId": "deleteTopPosts"
}