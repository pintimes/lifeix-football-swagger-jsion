{
    "summary": "获得用户的消息列表",
    "tags": 
    [
        "messages"
    ],

    "consumes": 
    [
        "application/json; charset=UTF-8"
    ],

    "produces": 
    [
        "application/json; charset=UTF-8"
    ],

    "description": "获得用户的消息列表",
    "parameters": 
    [
        {
            "$ref": "#/parameters/apiKeyYHeader"
        },

        {
            "name": "app",
            "in": "query",
            "type": "string",
            "required": true,
            "description": "应用：比如c-f,competition"
        },
        
        {
            "name": "categoryId",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "类目Id，如果传入categoryId，则返回categoryId={categoryId}的消息"
        },
        
        {
            "name": "type",
            "in": "query",
            "type": "string",
            "required": false,
            "description": "消息类型，如果传入type，则返回type={type}的消息"
        },

        {
            "name": "limit",
            "in": "query",
            "format": "int32",
            "type": "integer",
            "description": "每页数目，默认是每页20条"
        },

        {
            "name": "date",
            "in": "query",
            "format": "date-time",
            "type": "string",
            "description": "如果查最新消息不传即可；如果是分页查询则需将前一页最后一条记录的时间传入，通过时间定位"
        }
    ],

    "responses": 
    {
        "200": 
        {
            "schema": 
            {
                "type": "array",
                "items": 
                {
                    "$ref": "#/definitions/Message"
                }
            },

            "description": "Message列表"
        }
    },

    "operationId": "getMessages"
}