{
    "description": "方格，app端和web端用于首页展示，其中app端可以由用户选择性展示",
    "properties": 
    {
        "id": 
        {
            "description": "方格编号",
            "type": "string"
        },

        "name": 
        {
            "description": "方格名称或标题",
            "type": "string"
        },
        
        "image": 
        {
            "description": "图标或图片",
            "type": "string"
        },

        "labels": 
        {
            "description": "标签列表",
            "items": 
            {
                "$ref": "#/definitions/Label"
            },

            "type": "array"
        },
        
        "createTime": 
        {
            "description": "box创建日期",
            "type": "string",
            "format": "date-time"
        },

        "force": 
        {
            "description": "是否强制显示，为true时不允许删除",
            "type": "string"
        },

        "content": 
        {
            "description": "存放类型，目前只有一种类型'link'",
            "type": "string"
        },

        "link": 
        {
            "description": "链接对象，content='link'时该对象不为空",
            "$ref": "#/definitions/Link"
        }
    },

    "type": "object"
}