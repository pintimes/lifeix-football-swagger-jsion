{
    "summary": "获取banner下推广位和资讯列表",
    "tags": 
    [
        "获取推广位+资讯列表"
    ],

	"consumes": 
    [
        "application/x-www-form-urlencoded"
    ],
    
    "produces": 
    [
        "application/json; charset=UTF-8"
    ],
    
    "description": "获取banner下推广位和资讯列表",
    "parameters": 
    [
    	{
            "$ref": "#/parameters/apiKeyHeader"
        },
        {
            "description": "类目id，如：首页（homepage），教练员（coach）...",
            "name": "categoryId",
            "type": "string",
            "required": false,
            "default":"homepage",
            "in": "query"
        },
        {
            "description": "平台，ios/android",
            "name": "platform",
            "type": "string",
            "required": false,
            "default":"ios",
            "in": "query"
        },
        {
            "description": "版本号",
            "name": "version",
            "type": "string",
            "required": false,
            "default":"1",
            "in": "query"
        },
        {
            "description": "资讯时间(字符串)，格式：yyyy-MM-dd'T'HH:mm:ss.SSSXXX，例如：https://api.c-f.com/football/wemedia/posts/search?limit=19&date=2017-01-17T01:29:04.000Z&key=visitor",
            "name": "date",
            "type": "string",
            "required": false,
            "in": "query"
        },
         {
            "description": "资讯数量限制",
            "name": "limit",
            "type": "string",
            "format":"int32",
            "required": false,
            "default":10,
            "in": "query"
        },
         {
            "description": "根据某个属性的倒序排列资讯列表",
            "name": "orderBy",
            "type": "string",
            "required": false,
            "default":"createTime",
            "in": "query"
        }
    ],

    "responses": 
    {
        "200": 
        {
           "schema": 
            {
                "type": "array",
		    	"items": {
                	"$ref": "#/definitions/Box"
                }
            },
            "description": "推广位+资讯列表"
        }
    },

    "operationId": "getDefaultPromotionsList2"
}