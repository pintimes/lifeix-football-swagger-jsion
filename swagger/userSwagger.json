{
    "basePath": "/football",
    "paths": {
        "/user/users/weixin/web": {"post": {
            "summary": "web端微信用户注册/登录",
            "produces": ["application/json"],
            "description": "web端微信用户注册/登录，通过web前台传来的code获得access_token等信息，然后通过access_token等信息获得用户信息，然后存入数据库并返回给前台",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "webWeixinRegisterLoginUser",
            "parameters": [{
                "in": "formData",
                "name": "code",
                "description": "用户微信登录临时凭证，10分钟内有效，且使用一次后立刻失效",
                "type": "string",
                "required": true
            }],
            "tags": ["users"],
            "consumes": ["application/x-www-form-urlencoded"]
        }},
        "/user/users/{userId}/nickname": {"put": {
            "summary": "更新用户昵称",
            "produces": ["text/plain; charset=UTF-8"],
            "description": "更新用户昵称",
            "responses": {
                "200": {"description": "修改成功"},
                "401": {"description": "没有权限"}
            },
            "operationId": "updateUserNickname",
            "parameters": [
                {"$ref": "#/parameters/apiKeyYHeader"},
                {"$ref": "#/parameters/userPath"},
                {
                    "in": "formData",
                    "name": "label",
                    "description": "昵称",
                    "type": "string",
                    "required": true
                }
            ],
            "tags": ["users"],
            "consumes": ["application/x-www-form-urlencoded"]
        }},
        "/user/users/weixin/app": {"post": {
            "summary": "app端微信用户注册/登录",
            "produces": ["application/json"],
            "description": "app端微信用户注册/登录，通过客户端传来的access_token和openid等参数获得用户信息，然后存入数据库并返回给客户端",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "appWeixinRegisterLoginUser",
            "parameters": [
                {
                    "in": "formData",
                    "name": "device",
                    "format": "int32",
                    "description": "设备编号:1:web;2:ios;3:android",
                    "type": "integer",
                    "required": true
                },
                {
                    "in": "formData",
                    "name": "deviceId",
                    "description": "设备id",
                    "type": "string",
                    "required": true
                },
                {
                    "in": "formData",
                    "name": "access_token",
                    "description": "调用授权关系接口的调用凭证，2小时有效期",
                    "type": "string",
                    "required": true
                },
                {
                    "in": "formData",
                    "name": "refresh_token",
                    "description": "调用授权关系接口的调用凭证刷新凭证，30天有效期",
                    "type": "string",
                    "required": true
                },
                {
                    "in": "formData",
                    "name": "openid",
                    "description": "用户在微信中的用户id",
                    "type": "string",
                    "required": true
                },
                {
                    "in": "formData",
                    "name": "unionid",
                    "description": "跨平台的微信用户唯一id，唯一性高于openid",
                    "type": "string",
                    "required": true
                }
            ],
            "tags": ["users"],
            "consumes": ["application/x-www-form-urlencoded"]
        }},
        "/user/users/weibo/web": {"post": {
            "summary": "web端微博用户注册/登录",
            "produces": ["application/json"],
            "description": "微博用户注册/登录，通过前台传来的code获得access_token等信息，然后通过access_token等信息获得用户信息，然后存入数据库并返回给前台",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "webWeiboRegisterLoginUser",
            "parameters": [{
                "in": "formData",
                "name": "code",
                "description": "用户微博登录临时凭证，10分钟有效，使用一次后立刻失效",
                "type": "string",
                "required": true
            }],
            "tags": ["users"],
            "consumes": ["application/x-www-form-urlencoded"]
        }},
        "/user/users/{userId}/avatar": {"put": {
            "summary": "更新用户头像",
            "produces": ["text/plain; charset=UTF-8"],
            "description": "更新用户头像",
            "responses": {
                "200": {"description": "修改成功"},
                "401": {"description": "没有权限"}
            },
            "operationId": "updateUserAvatar",
            "parameters": [
                {"$ref": "#/parameters/apiKeyYHeader"},
                {"$ref": "#/parameters/userPath"},
                {
                    "in": "formData",
                    "name": "label",
                    "description": "头像",
                    "type": "string",
                    "required": true
                }
            ],
            "tags": ["users"],
            "consumes": ["application/x-www-form-urlencoded"]
        }},
        "/user/users/easyui": {"get": {
            "summary": "获得EasyUI形式的User列表",
            "description": "返回用户列表,该接口只能被管理员访问",
            "responses": {"200": {
                "schema": {
                    "type": "array",
                    "items": {"$ref": "#/definitions/User"}
                },
                "description": "User列表响应对象"
            }},
            "operationId": "getEasyUIUsers",
            "parameters": [
                {"$ref": "#/parameters/apiKeyYHeader"},
                {
                    "in": "query",
                    "name": "startTime",
                    "format": "date-time",
                    "description": "查询开始时间，格式yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "endTime",
                    "format": "date-time",
                    "description": "查询结束时间，格式yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
                    "type": "string",
                    "required": false
                },
                {
                    "default": 1,
                    "in": "query",
                    "name": "page",
                    "format": "int32",
                    "description": "页号",
                    "type": "integer",
                    "required": false
                },
                {
                    "default": 20,
                    "in": "query",
                    "name": "limit",
                    "format": "int32",
                    "description": "每页记录数",
                    "type": "integer",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "id",
                    "description": "Id",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "name",
                    "description": "用户名",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "email",
                    "description": "email",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "tel",
                    "description": "手机号码",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "gender",
                    "description": "性别",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "label",
                    "description": "角色标签",
                    "type": "string",
                    "required": false
                }
            ],
            "tags": ["users"]
        }},
        "/user/users/{userId}/label": {"put": {
            "summary": "更新用户角色标签",
            "produces": ["text/plain; charset=UTF-8"],
