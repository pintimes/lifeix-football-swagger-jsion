{
    "basePath": "/football",
    "paths": {
        "/user/users/easyui": {"get": {
            "summary": "获得EasyUI形式的User列表",
            "description": "返回用户列表,该接口只能被管理员访问",
            "responses": {"200": {
                "schema": {
                    "type": "array",
                    "items": {"$ref": "#/definitions/User"}
                },
                "description": "User列表响应对象"
            }},
            "operationId": "getEasyUIUsers",
            "parameters": [
                {"$ref": "#/parameters/apiKeyYHeader"},
                {
                    "in": "query",
                    "name": "startTime",
                    "format": "date-time",
                    "description": "查询开始时间，格式yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "endTime",
                    "format": "date-time",
                    "description": "查询结束时间，格式yyyy-MM-dd'T'HH:mm:ss.SSSXXX",
                    "type": "string",
                    "required": false
                },
                {
                    "default": 1,
                    "in": "query",
                    "name": "page",
                    "format": "int32",
                    "description": "页号",
                    "type": "integer",
                    "required": false
                },
                {
                    "default": 20,
                    "in": "query",
                    "name": "rows",
                    "format": "int32",
                    "description": "每页记录数",
                    "type": "integer",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "id",
                    "description": "Id",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "name",
                    "description": "用户名",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "email",
                    "description": "email",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "mobile",
                    "description": "手机号码",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "gender",
                    "description": "性别",
                    "type": "string",
                    "required": false
                },
                {
                    "in": "query",
                    "name": "label",
                    "description": "角色标签",
                    "type": "string",
                    "required": false
                }
            ],
            "tags": ["users"]
        }},
        "/user/users/{userId}": {"get": {
            "summary": "获取单个User",
            "produces": ["application/json; charset=UTF-8"],
            "description": "根据Id获取单个User对象",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "getUser",
            "parameters": [
                {"$ref": "#/parameters/apiKeyYHeader"},
                {"$ref": "#/parameters/userPath"}
            ],
            "tags": ["users"],
            "consumes": ["application/json; charset=UTF-8"]
        }},
        "/user/users/regist": {"post": {
            "summary": "User注册",
            "produces": ["application/json"],
            "description": "User注册",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "registUser",
            "parameters": [{
                "schema": {"$ref": "#/definitions/User"},
                "in": "body",
                "name": "user",
                "description": "普通用户",
                "required": true
            }],
            "tags": ["users"],
            "consumes": ["application/json"]
        }},
        "/user/users/weibo": {"post": {
            "summary": "微博用户注册/登录",
            "produces": ["application/json"],
            "description": "微博用户注册/登录",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "weiboRegistLoginUser",
            "parameters": [],
            "tags": ["users"],
            "consumes": ["application/json"]
        }},
        "/user/users/login": {"post": {
            "summary": "User登录",
            "produces": ["application/json"],
            "description": "User登录",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "loginUser",
            "parameters": [
                {
                    "in": "formData",
                    "name": "username",
                    "description": "用户名",
                    "type": "string",
                    "required": true
                },
                {
                    "in": "formData",
                    "name": "password",
                    "description": "密码密文，MD5加密",
                    "type": "string",
                    "required": true
                }
            ],
            "tags": ["users"],
            "consumes": ["application/json"]
        }},
        "/user/users/weixin": {"post": {
            "summary": "微信用户注册/登录",
            "produces": ["application/json"],
            "description": "微信用户注册/登录",
            "responses": {"200": {
                "schema": {"$ref": "#/definitions/User"},
                "description": "User对象"
            }},
            "operationId": "weixinRegistLoginUser",
            "parameters": [],
            "tags": ["users"],
            "consumes": ["application/json"]
        }}
    },
    "host": "api.c-f.com",
    "produces": ["application/json"],
    "responses": {},
    "schemes": [
        "http",
        "https"
    ],
    "parameters": {
        "apiKeyYHeader": {
            "default": "visitor",
            "in": "header",
            "name": "key",
            "description": "key",
            "type": "string",
            "required": true
        },
        "userPath": {
            "in": "path",
            "name": "userId",
            "description": "用户的Id",
            "type": "string",
            "required": true
        }
    },
    "definitions": {"User": {
        "description": "用户",
        "type": "object",
        "properties": {
            "birthday": {
                "format": "date-time",
                "description": "生日",
                "type": "string"
            },
            "gender": {
                "description": "性别",
                "type": "string"
            },
            "createTime": {
                "format": "date-time",
                "description": "注册时间",
                "type": "string"
            },
            "idCard": {
                "description": "身份证",
                "type": "string"
            },
            "nickname": {
                "description": "昵称",
                "type": "string"
            },
            "name": {
                "description": "名称",
                "type": "string"
            },
            "mobile": {
                "description": "手机",
                "type": "string"
            },
            "channel": {
                "description": "来源渠道",
                "type": "object",
                "properties": {
                    "name": {
                        "description": "来源名称：微信，微博",
                        "type": "string"
                    },
                    "id": {
                        "description": "来源Id，在微信中的Id",
                        "type": "string"
                    }
                }
            },
            "id": {
                "description": "id",
                "type": "string"
            },
            "avatar": {
                "description": "头像地址",
                "type": "string"
            },
            "label": {
                "description": "标签",
                "type": "string"
            },
            "email": {
                "description": "邮件",
                "type": "string"
            }
        }
    }},
    "swagger": "2.0",
    "info": {
        "contact": {
            "name": "曾光伟",
            "email": "guangweiz@l99.com"
        },
        "description": "中国足球网API - User系统",
